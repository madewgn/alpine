44,74fold
let &fdl = &fdl
let s:l = 47 - ((8 * winheight(0) + 18) / 37)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 47
let s:c = 49 - ((47 * winwidth(0) + 35) / 70)
if s:c > 0
  exe 'normal! ' . s:c . '|zs' . 49 . '|'
else
  normal! 049|
endif
let &g:so = s:so_save | let &g:siso = s:siso_save
